    </bean>
    
    <bean id="automatShiftPrintDetailService" class="com.mepsan.marwiz.automat.report.automatshiftreport.business.AutomatShiftPrintDetailService" lazy-init="true" scope="prototype" >
    </bean>
    
    <bean id="incomeExpenseReportDao" class="com.mepsan.marwiz.automat.report.incomeexpensereport.dao.IncomeExpenseReportDao"  lazy-init="true"  scope="request" >
        <property name="dataSource" ref="dataSource"/>
    </bean>
    
    <bean id="incomeExpenseReportService" class="com.mepsan.marwiz.automat.report.incomeexpensereport.business.IncomeExpenseReportService" lazy-init="true" scope="prototype" >
    </bean> 

</beans>
